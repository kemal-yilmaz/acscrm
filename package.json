{
  "name": "metronic-react-starter-kit",
  "version": "9.3.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build:staging": "cp .env.staging .env.local && next build",
    "lint": "eslint . --max-warnings=0",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky",
    "changeset": "changeset",
    "release": "changeset version && pnpm install --no-frozen-lockfile && git add -A && git commit -m \"chore(release): version packages\"",
    "publish:packages": "changeset publish",
    "commit": "cz",
    "lint-staged": "lint-staged",
    "test": "vitest run --coverage",
    "test:ui": "vitest",
    "test:watch": "vitest --watch",
    "test:e2e:install": "playwright install --with-deps",
    "test:e2e": "playwright test"
  },
  "dependencies": {
    "@dnd-kit/utilities": "^3.2.2",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/modifiers": "^9.0.0",
    "@dnd-kit/sortable": "^10.0.0",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "@headless-tree/core": "^1.4.0",
    "@headless-tree/react": "^1.4.0",
    "@hookform/resolvers": "^5.2.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.6.2",
    "@remixicon/react": "^4.6.0",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "@tanstack/react-table": "^8.21.3",
    "apexcharts": "4.7.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^4.1.0",
    "embla-carousel-autoplay": "8.6.0",
    "embla-carousel-react": "8.6.0",
    "input-otp": "^1.4.2",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.541.0",
    "framer-motion": "^12.23.24",
    "motion": "^12.23.12",
    "next": "15.5.0",
    "next-themes": "^0.4.6",
    "radix-ui": "^1.4.3",
    "react": "^19.1.1",
    "react-apexcharts": "1.7.0",
    "react-aria-components": "^1.12.0",
    "react-aspect-ratio": "^1.1.9",
    "react-day-picker": "^9.9.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.7.2",
    "react-is": "^19.1.1",
    "react-leaflet": "^5.0.0",
    "react-resizable-panels": "^3.0.5",
    "react-wrap-balancer": "^1.1.1",
    "recharts": "2.15.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.7",
    "vaul": "^1.1.2",
    "zod": "^4.1.3"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.10",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@types/leaflet": "^1.9.20",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "9.9.0",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.2.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "@playwright/test": "^1.49.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.6.1",
    "tailwindcss": "^4.1.12",
    "@vitest/coverage-v8": "^2.1.4",
    "jsdom": "^25.0.1",
    "vitest": "^2.1.4",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "**/*.{ts,tsx,js}": [
      "eslint --fix",
      "prettier --write"
    ],
    "**/*.{md,json,css}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
